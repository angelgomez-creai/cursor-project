{
    "name": "ecommerce-frontend",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "packageManager": "pnpm@8.15.0",
    "scripts": {
      "dev": "vite",
      "build": "tsc && vite build",
      "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
      "preview": "vite preview",
      "test": "jest",
      "test:watch": "jest --watch",
      "test:coverage": "jest --coverage",
      "test:ci": "jest --ci --coverage --maxWorkers=2",
      "docs": "typedoc",
      "docs:watch": "nodemon --watch src --ext ts,tsx --exec \"pnpm run docs\"",
      "docs:serve": "serve docs -l 3001",
      "docs:build": "typedoc && pnpm run docs:generate-examples",
      "docs:generate-examples": "node scripts/generate-examples.js",
      "docs:validate": "node scripts/validate-docs.js",
      "docs:watch-script": "node scripts/watch-docs.js",
      "cypress:open": "cypress open",
      "cypress:run": "cypress run",
      "cypress:run:ci": "cypress run --headless --browser chrome"
    },
    "dependencies": {
      "react": "^18.2.0",
      "react-dom": "^18.2.0",
      "react-router-dom": "^6.8.0",
      "axios": "^1.6.0",
      "antd": "^5.12.0",
      "@ant-design/icons": "^5.2.6"
    },
    "devDependencies": {
      "@types/react": "^18.2.43",
      "@types/react-dom": "^18.2.17",
      "@typescript-eslint/eslint-plugin": "^6.14.0",
      "@typescript-eslint/parser": "^6.14.0",
      "@vitejs/plugin-react": "^4.2.1",
      "eslint": "^8.55.0",
      "eslint-plugin-react-hooks": "^4.6.0",
      "eslint-plugin-react-refresh": "^0.4.5",
      "typescript": "^5.2.2",
      "vite": "^5.0.8",
      "@testing-library/react": "^14.1.2",
      "@testing-library/jest-dom": "^6.1.5",
      "@testing-library/user-event": "^14.5.1",
      "@types/jest": "^29.5.11",
      "jest": "^29.7.0",
      "jest-environment-jsdom": "^29.7.0",
      "@jest/globals": "^29.7.0",
      "ts-jest": "^29.1.1",
      "identity-obj-proxy": "^3.0.0",
      "typedoc": "^0.25.4",
      "typedoc-plugin-markdown": "^3.17.1",
      "typedoc-plugin-no-inherit": "^1.7.0",
      "nodemon": "^3.0.2",
      "serve": "^14.2.1",
      "cypress": "^13.6.0",
      "@testing-library/react": "^14.1.2",
      "@testing-library/user-event": "^14.5.1",
      "@testing-library/jest-dom": "^6.1.5"
    }
  }
